{
    "name": "jobcerto/countable",
    "description": "A Laravel countable Field",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Jobcerto\\Countable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jobcerto\\Countable\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
